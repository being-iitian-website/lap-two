    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "prisma": "^6.19.1",
    "serverless-http": "^4.0.0",
    "swagger-ui": "^5.31.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
